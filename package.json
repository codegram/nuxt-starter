{
  "name": "nuxt-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 5000",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "component": "cypress open-ct --browser chrome",
    "component:ci": "cypress run-ct --browser chrome --headless",
    "storybook": "nuxt storybook",
    "storybook:build": "nuxt storybook build"
  },
  "dependencies": {
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.28.1"
  },
  "engines": {
    "node": ">=15 <16",
    "npm": ">=7 <8"
  },
  "devDependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.1.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-a11y": "^6.3.11",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/vue": "^5.8.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.5",
    "core-js": "^3.18.2",
    "cypress": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.19.1",
    "jest": "^27.2.5",
    "nuxt-svg-loader": "^1.2.0",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-footnotes": "^4.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "vue-jest": "^3.0.7"
  }
}
